{
  "extends": "../generic-tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": ["@types/node"],
  },
  "include": ["src"],
  "references": [
    {"path": "../service-worker"},
    {"path": "../components"},
  ]
}